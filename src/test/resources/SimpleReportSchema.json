                "hashAlgo": {
                  "type": "string",
                  "minLength": 1
                }
              }
            },
            "signatures": {
              "type": "array",
              "minItems": 1,
              "items": {
                "type": "object",
                "properties": {
                  "signatureFormat": {
                    "type": "string",
                    "minLength": 1
